{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/vercel_wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "build_files.sh",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "staticfiles"
      }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dashboard/vercel_wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "dashboard.settings",
    "PYTHONPATH": "/vercel/path0"
  },
  "functions": {
    "dashboard/vercel_wsgi.py": {
      "includeFiles": "**"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/dashboard/vercel_wsgi.py"
    }
  ]
}