{
  "version": 2,
  "builds": [
    {
      "src": "dashboard/vercel_wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    }
  ],
  "installCommand": "pip install -r requirements.txt",
  "env": {
    "DJANGO_SETTINGS_MODULE": "dashboard.settings",
    "PYTHONPATH": "/vercel/path0"
  },
  "rewrites": [
    {
      "source": "/static/(.*)",
      "destination": "/staticfiles/$1"
    },
    {
      "source": "/media/(.*)",
      "destination": "/media/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/dashboard/vercel_wsgi.py"
    },
    {
      "source": "/(.*)",
      "destination": "/dashboard/vercel_wsgi.py"
    }
  ]
}